[core]
	quotepath = false
[color]
	ui = auto
[alias]
	a = add
	ap = add -p
	au = add -u
	b = branch
	ba = branch -a
	c = commit
	ci = commit
	ca = commit --amend -C HEAD
	cam = commit --amend -m
	car = commit -C HEAD --amend --reset-author
	cb = checkout -b
	cm = commit -m
	co = checkout
	d = diff
	dc = diff --cached
	ft = fetch --tags
	h = checkout --
	hd = checkout --
	l = log --decorate
	lt = log --decorate --stat
	lo = log --decorate --oneline
	lg = log --decorate --oneline --graph
	mg = merge
	pl = pull
	pr = pull --rebase
	ps = push
	pt = push --tags
	rc = rebase --continue
	ri = rebase -i
	s = status
	ss = status -s
	st = status
	sc = svn clone --no-minimize-url --prefix svn/ -s
